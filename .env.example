# LiveKit configuration
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=
LIVEKIT_URL=wss://your-instance.livekit.cloud
LIVEKIT_ROOM_PREFIX=betsy-classroom  # Prefix for generated LiveKit room names

# OpenAI configuration
OPENAI_API_KEY=

# Server settings
PORT=8000  # Port for the FastAPI server
HOST=0.0.0.0  # Host for the FastAPI server

# CLM (Custom Log Manager) Configuration
CLM_URL="http://localhost:9999"

# Agent Default Operational Settings
AGENT_DEFAULT_VOICE="alloy"
AGENT_DEFAULT_TEMPERATURE="0.8"
AGENT_DEFAULT_MAX_RESPONSE_TOKENS="1024"
AGENT_DEFAULT_MODALITIES="text_and_audio"  # Options: "text_and_audio", "text_only", "audio_only"
